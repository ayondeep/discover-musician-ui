<h2>{{title}}</h2>


<table class="table table-striped table-dark">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Type</th>
      <th scope="col">Date of Birth</th>
    </tr>
  </thead>
  <tbody>
  {{#each people as |person|}}
    <tr>
      <td>{{person.firstName}} {{person.lastName}}</td>
      <td>{{person.musicianType}}</td>
      <td>{{person.dob}}</td>
    </tr>
  {{/each}}
  </tbody>
</table>
